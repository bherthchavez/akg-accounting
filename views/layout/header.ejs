                            </span>
                            <!-- <span class="badge badge-pill bg-red">6</span> -->
                            <%}%>
                    </a>
                    <div class="dropdown-menu dropdown-menu-arrow dropdown-menu-end dropdown-menu-card">
                      <div class="card " style="height: 25rem; width: 25rem">
                        <div class="card-header">
                          <h3 class="card-title">Notifications</h3>
                        </div>

                        <div
                          class="list-group list-group-flush list-group-hoverable card-body-scrollable card-body-scrollable-shadow">
                          <%if (nav.notif.expenPending && user.role===1) {%>
                            <% nav.notif.expenPending.forEach(function(notif){ %>
                              <div class="list-group-item">
                                <div class="row align-items-center">
                                  <div class="col-auto"><span
                                      class="status-dot status-dot-animated bg-orange d-block"></span>
                                  </div>
                                  <div class="col text-truncate">
                                    <a href="/pending-expenses" class="text-body d-block fw-bold">Expenses For Approvals
                                    </a>
                                    <div class="d-block text-muted text-truncate mt-n1 fs-h5">
                                      Created by: <strong>
                                        <%=notif.created_by%>,
                                      </strong> Amount: <strong>
                                        <%= (notif.amount).toFixed(2).replace(/\d(?=(\d{3})+\.)/g, '$&,' ) %>
                                      </strong> QAR
                                    </div>
                                  </div>
                                  <div class="col-auto">
                                    <!-- <a href="#" class="list-group-item-actions">View -->
                                    <!-- </a> -->
                                  </div>
                                </div>
                              </div>
                              <% }) %>
                                <%}%>


                                  <%if (nav.notif.exIstimara) {%>
                                    <% nav.notif.exIstimara.forEach(function(notif){ %>
                                      <div class="list-group-item">
                                        <div class="row align-items-center">
                                          <div class="col-auto"><span
                                              class="status-dot status-dot-animated bg-red d-block"></span>
                                          </div>
                                          <div class="col text-truncate">
                                            <a href="/vehicle-list" class="text-body d-block fw-bold">Vehicle Istimara
                                              Expired </a>
                                            <div class="d-block text-muted text-truncate mt-n1 fs-h5">
                                              Vehicle No: <strong>
                                                <%=notif.vehicle_no%>,
                                              </strong> Expiry Date: <strong>
                                                <%=(new Date(notif.istimara_exdate).toLocaleDateString("en-US",
                                                  {year: 'numeric' , day: 'numeric' ,month: 'short' })) %>
                                              </strong>
                                            </div>
                                          </div>
                                          <div class="col-auto">
                                            <!-- <a href="#" class="list-group-item-actions">View -->
                                            <!-- </a> -->
                                          </div>
                                        </div>
                                      </div>
                                      <% }) %>
                                        <%}%>

                                          <%if (nav.notif.exInsurance) {%>
                                            <% nav.notif.exInsurance.forEach(function(notif){ %>
                                              <div class="list-group-item">
                                                <div class="row align-items-center">
                                                  <div class="col-auto"><span
                                                      class="status-dot status-dot-animated bg-red d-block"></span>
                                                  </div>
                                                  <div class="col text-truncate">
                                                    <a href="/vehicle-list" class="text-body d-block fw-bold">Vehicle
                                                      Insurance Expired </a>
                                                    <div class="d-block text-muted text-truncate mt-n1 fs-h5">
                                                      Vehicle No: <strong>
                                                        <%=notif.vehicle_no%>,
                                                      </strong> Expiry Date: <strong>
                                                        <%=(new Date(notif.insurance_exdate).toLocaleDateString("en-US",
                                                          {year: 'numeric' , day: 'numeric' ,month: 'short' })) %>
                                                      </strong>
                                                    </div>
                                                  </div>
                                                  <div class="col-auto">
                                                    <!-- <a href="#" class="list-group-item-actions">View -->
                                                    <!-- </a> -->
                                                  </div>
                                                </div>
                                              </div>
                                              <% }) %>
                                                <%}%>

                                                  <%if (nav.notif.exQID) {%>
                                                    <% nav.notif.exQID.forEach(function(notif){ %>

                                                      <div class="list-group-item">
                                                        <div class="row align-items-center">
                                                          <div class="col-auto"><span
                                                              class="status-dot status-dot-animated bg-red d-block"></span>
                                                          </div>
                                                          <div class="col text-truncate">
                                                            <a href="/employee-list"
                                                              class="text-body d-block fw-bold">Employee QID Expired
                                                            </a>
                                                            <div class="d-block text-muted text-truncate mt-n1 fs-h5">
                                                              Name: <strong>
                                                                <%=notif.fname%>
                                                                  <%=notif.lname%>,
                                                              </strong> Expiry Date: <strong>
                                                                <%=(new Date(notif.ex_qid).toLocaleDateString("en-US",
                                                                  {year: 'numeric' , day: 'numeric' ,month: 'short' }))
                                                                  %>
                                                              </strong>
                                                            </div>
                                                          </div>
                                                          <div class="col-auto">
                                                            <!-- <a href="#" class="list-group-item-actions">View -->
                                                            <!-- </a> -->
                                                          </div>
                                                        </div>
                                                      </div>
                                                      <% }) %>
                                                        <%}%>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>


                <div class="nav-item dropdown">
                  <a href="#" class="nav-link d-flex lh-1 text-reset p-0" data-bs-toggle="dropdown"
                    aria-label="Open user menu">
                    <span class="avatar avatar-sm"
                      style="background-image: url(../static/avatars/user-avatar.png)"></span>
                    <div class="d-none d-xl-block ps-2">
                      <div>
                        <%= user.userName %>
                      </div>
                      <div class="mt-1 small text-muted">
                        <%= user.userPosition %>
                      </div>
                    </div>
                  </a>
                  <div class="dropdown-menu dropdown-menu-end dropdown-menu-arrow">
                    <%- (user.role)==1 ? '<a href="/hima-the-hokage" class="dropdown-item">Settings</a>' : '' %>
                      <a href="/logout" class="dropdown-item">Logout</a>
                  </div>
                </div>

          </div>
      </header>

      <% if (message && message.type !='transac' && message.type !='rentInfo' && message.type !='viewVoucher' &&
        message.type !='viewInvoice' ) { %>
        <div class="alert-notif2">
          <div class="alert alert-important alert-<%= message.type %> alert-dismissible alertNotif2" role="alert">
            <div class="d-flex">
              <div class="fw-light">
                <% if (message.type==='danger' ){ %>
                  <i class="ti ti-alert-triangle fs-h2 me-2"></i>
                  <% }else{ %>
                    <i class="ti ti-check fs-h2 me-2"></i>
                    <% } %>
                      <%= message.message %>
              </div>
            </div>
            <a class="btn-close btn-close-white" data-bs-dismiss="alert" aria-label="close"></a>
          </div>
        </div>

        <% } %>

          <% if (nav.view===2) { %>

            <div class="navbar-expand-md">
              <div class="collapse navbar-collapse" id="navbar-menu">
                <div class="navbar navbar-light">
                  <div class="container-xl">
                    <ul class="navbar-nav">
                      <li class="nav-item <%- (nav.title) === 'Dashboard' ? 'active' : ''  %> ">
                        <a class="nav-link" href="/">
                          <span class="nav-link-icon d-md-none d-lg-inline-block">
                            <i class="ti ti-home-stats fs-h2"></i>
                          </span>
                          <span class="nav-link-title"> Dashboard </span>
                        </a>
                      </li>
                      <li class="nav-item dropdown <%- (nav.title) == 'Accounts' ? 'active' : ''  %> ">
                        <a class="nav-link dropdown-toggle" href="#navbar-help" data-bs-toggle="dropdown"
                          data-bs-auto-close="outside" role="button" aria-expanded="false">
                          <span class="nav-link-icon d-md-none d-lg-inline-block">
                            <i class="ti ti-presentation-analytics fs-h2"></i>
                          </span>
                          <span class="nav-link-title">
                            Accounts
                          </span>
                        </a>
                        <div class="dropdown-menu ">
                          <!-- <a class="dropdown-item <%- (nav.title) == 'Accounts' && (nav.child) == 'Bank' ?  'active' : ''  %>" href="/bank-accounts">Bank </a> -->
                          <a class="dropdown-item <%- (nav.title) == 'Accounts' && (nav.child) == 'Vehicle' ?  'active' : ''  %>"
                            href="/vehicle-list">Vehicles</a>
                          <a class="dropdown-item <%- (nav.title) == 'Accounts' && (nav.child) == 'Employee' ?  'active' : ''  %>"
                            href="/employee-list">Employees</a>
                        </div>
                      </li>
                      <li class="nav-item dropdown <%- (nav.title) == 'Settings' ? 'active' : ''  %>">
                        <a class="nav-link dropdown-toggle" href="#navbar-help" data-bs-toggle="dropdown"
                          data-bs-auto-close="outside" role="button" aria-expanded="false">
                          <span class="nav-link-icon d-md-none d-lg-inline-block">
                            <i class="ti ti-adjustments-horizontal fs-h2"></i>
                          </span>
                          <span class="nav-link-title">Settings</span>
                        </a>
                        <div class="dropdown-menu">
                          <a class="dropdown-item <%- (nav.title) == 'Settings' && (nav.child) == 'Master' ?  'active' : ''  %>"
                            href="/master">Master </a>
                          <a class="dropdown-item <%- (nav.title) == 'Settings' && (nav.child) == 'System Settings' ?  'active' : ''  %>"
                            href="/system-settings">System Setup</a>
                        </div>
                      </li>
                    </ul>
                    <% if (nav.title==='Dashboard' ) { %>
                      <div class="my-2 my-md-0 flex-grow-1 flex-md-grow-0 order-first order-md-last">
                        <form action="../" method="get" autocomplete="off" novalidate>
                          <div class="input-icon">
                            <span class="input-icon-addon">
                              <i class="ti ti-search"></i>
                            </span>
                            <input type="text" id="searchTrans" class="form-control" placeholder="Search transaction…"
                              aria-label="Search transaction">
                          </div>
                        </form>
                      </div>
                      <%}%>

                  </div>
                </div>
              </div>
            </div>
            <% }else{ %>
              <div class="navbar-expand-md">
                <div class="collapse navbar-collapse" id="navbar-menu">
                  <div class="navbar navbar-light">
                    <div class="container-xl">
                      <ul class="navbar-nav">
                        <li class="nav-item <%- (nav.title) === 'Dashboard' ? 'active' : ''  %> ">
                          <a class="nav-link" href="/owner-dashboard">
                            <span class="nav-link-icon d-md-none d-lg-inline-block">
                              <i class="ti ti-home-stats fs-h2"></i>
                            </span>
                            <span class="nav-link-title"> Dashboard </span>
                          </a>
                        </li>

                        <li class="nav-item dropdown <%- (nav.title) == 'Company' ? 'active' : ''  %> ">
                          <a class="nav-link dropdown-toggle" href="#navbar-help" data-bs-toggle="dropdown"
                            data-bs-auto-close="outside" role="button" aria-expanded="false">
                            <span class="nav-link-icon d-md-none d-lg-inline-block">
                              <i class="ti ti-building-skyscraper fs-h2"></i>
                            </span>
                            <span class="nav-link-title">
                              List of Companies
                            </span>
                          </a>
                          <div class="dropdown-menu ">
                            <a class="dropdown-item <%- (nav.title) == 'Company' && (nav.subTtile) == 'Subsidiaries' ? 'active' : ''  %>"
                              href="/company-setup">Subsidiaries </a>
                            <a class="dropdown-item <%- (nav.title) == 'Company' && (nav.subTtile) == 'Sponsored' ?  'active' : ''  %>"
                              href="/company-sponsored">Sponsored</a>
                          </div>
                        </li>

                        <li class="nav-item <%- (nav.title) === 'User' ? 'active' : ''  %> ">
                          <a class="nav-link" href="/user-setup">
                            <span class="nav-link-icon d-md-none d-lg-inline-block">
                              <i class="ti ti-users fs-h2"></i>
                            </span>
                            <span class="nav-link-title"> User Management</span>
                          </a>
                        </li>
                      </ul>
                    </div>
                  </div>
                </div>
              </div>


              <% } %>


    </div>



    <!-- Transaction Notification -->
    <% if (message && message.type==='transac' ) { %>
      <div class="modal modal-blur fade" id="modal-success-trans" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-sm modal-dialog-centered" role="document">
          <div class="modal-content">
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            <div class="modal-status bg-success"></div>
            <div class="modal-body text-center py-4">
              <i class="ti ti-circle-check text-green fs--h1"></i>
              <% if (message.tType==='voucherV' ){ %>
                <h3 mt-2>Voucher Created</h3>
                <% }else if(message.tType==='voucherU' ){ %>
                  <h3 mt-2>Voucher Updated</h3>
                  <% }else if(message.tType==='invoice' ){ %>
                    <h3 mt-2>Invoice Created</h3>
                    <% }else if(message.tType==='invUpdate' ){ %>
                      <h3 mt-2>Invoice Updated</h3>
                      <% }else{ %>
                        <h3 mt-2>Voucher Updated</h3>
                        <% } %>
                          <div class="text-muted">
                            <%= message.message %>
                          </div>
            </div>
            <div class="modal-footer">
              <div class="w-100">
                <div class="row">
                  <div class="col">
                    <a href="/" class="btn w-100">
                      Go to dashboard
                    </a>
                  </div>

                  <% if (message.tType==='voucherV' ){ %>
                    <div class="col">
                      <a href="/rent-info/<%= message.transID %>" class="btn btn-success w-100">
                        View voucher
                      </a>

                    </div>
                    <% }else if(message.tType==='voucher' || message.tType==='voucherU' ){ %>
                      <div class="col">
                        <a href="/view-voucher/<%= message.transID %>" class="btn btn-success w-100">
                          View voucher
                        </a>
                      </div>
                      <% }else{ %>
                        <div class="col">
                          <a href="/view-invoice/<%= message.transID %>" class="btn btn-success w-100">
                            View invoice
                          </a>
                        </div>
                        <% } %>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <% } %>
        <script>
          $(document).ready(function () {
            setTimeout(function () {
              $(".alertNotif2").alert('close');
            }, 5000);
            '<% if (message && message.type ==="transac") { %>'
            $("#modal-success-trans").modal('toggle');
            ' <% }else if (message && message.type ==="viewVoucher")  { %>'
            $("#modal-viewVoucher").modal('toggle');
            ' <% } %>'
          });

        </script>