                    <i class="ti ti-sun-high fs-h2"></i>
                  </a>

                  <div class="nav-item dropdown d-none d-md-flex me-3">
                    <a href="#" class="nav-link px-0" data-bs-toggle="dropdown" tabindex="-1"
                      aria-label="Show notifications">
                      <i class="ti ti-bell icon"></i>
                      <% console.log("error: nav.notif.expenPending.length") %>

                        <%if (nav.notif.expenPending.length && user.role===1) {%>
                          <span class="badge badge-pill bg-red">
                            <%= nav.notif.expenPending.length + nav.notif.exIstimara.length +
                              nav.notif.exInsurance.length + nav.notif.exQID.length %>
                          </span>
                          <%}else if(nav.notif.exIstimara.length || nav.notif.exInsurance.length ||
                            nav.notif.exQID.length) {%>
                            <span class="badge badge-pill bg-red">
