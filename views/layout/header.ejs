<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover" />
  <meta http-equiv="X-UA-Compatible" content="ie=edge" />
  <title>
    <%= title %>
  </title>
  <!-- CSS files -->
  <link href="../dist/css/custom.css" rel="stylesheet" />
  <link href="../dist/css/tabler.min.css" rel="stylesheet" />
  <link href="../dist/css/tabler-flags.min.css" rel="stylesheet" />
  <link href="../dist/css/tabler-payments.min.css" rel="stylesheet" />
  <link href="../dist/css/tabler-vendors.min.css" rel="stylesheet" />
  <link href="../dist/css/demo.min.css" rel="stylesheet" />
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@tabler/icons@latest/iconfont/tabler-icons.min.css">
  <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.24.0/moment.min.js"></script>
  <style>
    @import url('https://rsms.me/inter/inter.css');

    :root {
      --tblr-font-sans-serif: Inter, -apple-system, BlinkMacSystemFont, San Francisco, Segoe UI, Roboto, Helvetica Neue, sans-serif;
    }
  </style>
</head>

<body>
  <!--class="layout-fluid" -->
  <script src="../dist/js/demo-theme.min.js"></script>
  <div class="page">
    <div class="sticky-top">
      <header class="navbar navbar-expand-md navbar-light sticky-top d-print-none">
        <div class="container-xl">
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbar-menu">
            <span class="navbar-toggler-icon"></span>
          </button>

          <h1 class="navbar-brand navbar-brand-autodark d-none-navbar-horizontal pe-0 pe-md-3">
            <a href=".">
              <!-- <img src="./static/logo.svg" width="110" height="32" alt="Tabler" class="navbar-brand-image"> -->
              <!-- <img src="../static/illustrations/EL.png" width="210" height="42" alt="Tabler" class="navbar-brand-image"> -->
              <!-- <h5 class="fw-bolder text-muted">مجـــــمـــــوعـــــة الــــــنــــــائــــــل الــــــقــــــابــــــضـــــة ذ.م.م</h5> 
                <h4 class="fw-bolder text-muted">EL NAAEL GROUP HOLDING CO. W.L.L</h4>  -->
              <h3 class="fw-bolder text-yellow "><%- (nav.view) === 2 ?  user.company : 'AL KATHIRI GROUPS' %></h3>
                                               
            </a>
          </h1>
          <div class="navbar-nav flex-row order-md-last">
            <% if (user.role === 1  ) { %>
            <div class="nav-item d-none d-md-flex me-3">
              <div class="btn-list">
                <a href="/owner-dashboard" class="btn">
                  <i class="ti ti-server-cog fs-h3 me-1"></i>
                  Setup
                </a>
              </div>
            </div>
            <%}%>

            <div class="d-none d-md-flex">

              <a href="?theme=dark" class="nav-link px-0 hide-theme-dark" title="Enable dark mode"
                data-bs-toggle="tooltip" data-bs-placement="bottom">
                <i class="ti ti-moon fs-h2"></i>
              </a>
              <a href="?theme=light" class="nav-link px-0 hide-theme-light" title="Enable light mode"
                data-bs-toggle="tooltip" data-bs-placement="bottom">
                <i class="ti ti-sun-high fs-h2"></i>
              </a>

              <div class="nav-item dropdown d-none d-md-flex me-3">
                <a href="#" class="nav-link px-0" data-bs-toggle="dropdown" tabindex="-1"
                  aria-label="Show notifications">
                  <!-- Download SVG icon from http://tabler-icons.io/i/bell -->
                  <svg xmlns="http://www.w3.org/2000/svg" class="icon" width="24" height="24" viewBox="0 0 24 24"
                    stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                    <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                    <path d="M10 5a2 2 0 0 1 4 0a7 7 0 0 1 4 6v3a4 4 0 0 0 2 3h-16a4 4 0 0 0 2 -3v-3a7 7 0 0 1 4 -6" />
                    <path d="M9 17v1a3 3 0 0 0 6 0v-1" />
                  </svg>
                  <span class="badge bg-red"></span>
                </a>
                <div class="dropdown-menu dropdown-menu-arrow dropdown-menu-end dropdown-menu-card">
                  <div class="card">
                    <div class="card-header">
                      <h3 class="card-title">Last updates</h3>
                    </div>
                    <div class="list-group list-group-flush list-group-hoverable">
                      <div class="list-group-item">
                        <div class="row align-items-center">
                          <div class="col-auto"><span class="status-dot status-dot-animated bg-red d-block"></span>
                          </div>
                          <div class="col text-truncate">
                            <a href="#" class="text-body d-block">Example 1</a>
                            <div class="d-block text-muted text-truncate mt-n1">
                              Change deprecated html tags to text decoration classes (#29604)
                            </div>
                          </div>
                          <div class="col-auto">
                            <a href="#" class="list-group-item-actions">
                              <!-- Download SVG icon from http://tabler-icons.io/i/star -->
                              <svg xmlns="http://www.w3.org/2000/svg" class="icon text-muted" width="24" height="24"
                                viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none"
                                stroke-linecap="round" stroke-linejoin="round">
                                <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                                <path
                                  d="M12 17.75l-6.172 3.245l1.179 -6.873l-5 -4.867l6.9 -1l3.086 -6.253l3.086 6.253l6.9 1l-5 4.867l1.179 6.873z" />
                              </svg>
                            </a>
                          </div>
                        </div>
                      </div>
                      <div class="list-group-item">
                        <div class="row align-items-center">
                          <div class="col-auto"><span class="status-dot d-block"></span></div>
                          <div class="col text-truncate">
                            <a href="#" class="text-body d-block">Example 2</a>
                            <div class="d-block text-muted text-truncate mt-n1">
                              justify-content:between ⇒ justify-content:space-between (#29734)
                            </div>
                          </div>
                          <div class="col-auto">
                            <a href="#" class="list-group-item-actions show">
                              <!-- Download SVG icon from http://tabler-icons.io/i/star -->
                              <svg xmlns="http://www.w3.org/2000/svg" class="icon text-yellow" width="24" height="24"
                                viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none"
                                stroke-linecap="round" stroke-linejoin="round">
                                <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                                <path
                                  d="M12 17.75l-6.172 3.245l1.179 -6.873l-5 -4.867l6.9 -1l3.086 -6.253l3.086 6.253l6.9 1l-5 4.867l1.179 6.873z" />
                              </svg>
                            </a>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>


            <div class="nav-item dropdown">
              <a href="#" class="nav-link d-flex lh-1 text-reset p-0" data-bs-toggle="dropdown"
                aria-label="Open user menu">
                <span class="avatar avatar-sm" style="background-image: url(../static/avatars/user-avatar.png)"></span>
                <div class="d-none d-xl-block ps-2">
                  <div>
                    <%= user.userName %>
                  </div>
                  <div class="mt-1 small text-muted">
                    <%= user.userPosition %>
                  </div>
                </div>
              </a>
              <div class="dropdown-menu dropdown-menu-end dropdown-menu-arrow">
                <%- (user.role)==1 ? '<a href="/hima-the-hokage" class="dropdown-item">Settings</a>' : '' %>
                  <a href="/logout" class="dropdown-item">Logout</a>
              </div>
            </div>

          </div>
      </header>

      <% if (message && message.type !='transac' && message.type !='rentInfo' ) { %>
        <div class="alert-notif2">
          <div class="alert alert-important alert-<%= message.type %> alert-dismissible alertNotif2" role="alert">
            <div class="d-flex">
              <div class="fw-light">
                <% if (message.type==='danger' ){ %>
                  <i class="ti ti-alert-triangle fs-h2 me-2"></i>
                  <% }else{ %>
                    <i class="ti ti-check fs-h2 me-2"></i>
                    <% } %>
                      <%= message.message %>
              </div>
            </div>
            <a class="btn-close btn-close-white" data-bs-dismiss="alert" aria-label="close"></a>
          </div>
        </div>
        <% } %>
        
        <% if (nav.view === 2) { %>
         
          <div class="navbar-expand-md">
            <div class="collapse navbar-collapse" id="navbar-menu">
              <div class="navbar navbar-light">
                <div class="container-xl">
                  <ul class="navbar-nav">
                    <li class="nav-item <%- (nav.title) === 'Dashboard' ? 'active' : ''  %> ">
                      <a class="nav-link" href="/">
                        <span class="nav-link-icon d-md-none d-lg-inline-block">
                          <i class="ti ti-home-stats fs-h2"></i>
                        </span>
                        <span class="nav-link-title"> Dashboard </span>
                      </a>
                    </li>
                    <li class="nav-item dropdown <%- (nav.title) == 'Accounts' ? 'active' : ''  %> ">
                      <a class="nav-link dropdown-toggle" href="#navbar-help" data-bs-toggle="dropdown"
                        data-bs-auto-close="outside" role="button" aria-expanded="false">
                        <span class="nav-link-icon d-md-none d-lg-inline-block">
                          <i class="ti ti-presentation-analytics fs-h2"></i>
                        </span>
                        <span class="nav-link-title">
                          Accounts
                        </span>
                      </a>
                      <div class="dropdown-menu ">
                        <!-- <a class="dropdown-item <%- (nav.title) == 'Accounts' && (nav.child) == 'Bank' ?  'active' : ''  %>" href="/bank-accounts">Bank </a> -->
                        <a class="dropdown-item <%- (nav.title) == 'Accounts' && (nav.child) == 'Vehicle' ?  'active' : ''  %>"
                          href="/vehicle-list">Vehicle List</a>
                      </div>
                    </li>
                    <li class="nav-item dropdown <%- (nav.title) == 'Settings' ? 'active' : ''  %>">
                      <a class="nav-link dropdown-toggle" href="#navbar-help" data-bs-toggle="dropdown"
                        data-bs-auto-close="outside" role="button" aria-expanded="false">
                        <span class="nav-link-icon d-md-none d-lg-inline-block">
                          <i class="ti ti-adjustments-horizontal fs-h2"></i>
                        </span>
                        <span class="nav-link-title">Settings</span>
                      </a>
                      <div class="dropdown-menu">
                        <a class="dropdown-item <%- (nav.title) == 'Settings' && (nav.child) == 'Master' ?  'active' : ''  %>"
                          href="/master">Master </a>
                        <a class="dropdown-item <%- (nav.title) == 'Settings' && (nav.child) == 'System Settings' ?  'active' : ''  %>"
                          href="/system-settings">System Settings</a>
                      </div>
                    </li>
                  </ul>
                </div>
              </div>
            </div>
          </div>
          <% }else{ %>
            <div class="navbar-expand-md">
              <div class="collapse navbar-collapse" id="navbar-menu">
                <div class="navbar navbar-light">
                  <div class="container-xl">
                    <ul class="navbar-nav">
                      <li class="nav-item <%- (nav.title) === 'Dashboard' ? 'active' : ''  %> ">
                        <a class="nav-link" href="/owner-dashboard">
                          <span class="nav-link-icon d-md-none d-lg-inline-block">
                            <i class="ti ti-home-stats fs-h2"></i>
                          </span>
                          <span class="nav-link-title"> Dashboard </span>
                        </a>
                      </li>
                      <li class="nav-item <%- (nav.title) === 'Setup' ? 'active' : ''  %> ">
                        <a class="nav-link" href="/company-setup">
                          <span class="nav-link-icon d-md-none d-lg-inline-block">
                            <i class="ti ti-building-skyscraper fs-h2"></i>
                          </span>
                          <span class="nav-link-title"> Company </span>
                        </a>
                      </li>
                      <li class="nav-item <%- (nav.title) === 'User' ? 'active' : ''  %> ">
                        <a class="nav-link" href="/user-setup">
                          <span class="nav-link-icon d-md-none d-lg-inline-block">
                            <i class="ti ti-users fs-h2"></i>
                          </span>
                          <span class="nav-link-title"> User </span>
                        </a>
                      </li>
                    </ul>
                  </div>
                </div>
              </div>
            </div>
          

          <% } %>
    </div>




    <% if (message && message.type==='transac' ) { %>
      <div class="modal modal-blur fade" id="modal-success-trans" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-sm modal-dialog-centered" role="document">
          <div class="modal-content">
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            <div class="modal-status bg-success"></div>
            <div class="modal-body text-center py-4">
              <i class="ti ti-circle-check text-green fs--h1"></i>
              <% if (message.tType==='voucher' ){ %>
                <h3 mt-2>Voucher Created</h3>
                <% }else if(message.tType==='invoice' ){ %>
                  <h3 mt-2>Invoice Created</h3>
                  <% }else{ %>
                    <h3 mt-2>Vehicle Returned</h3>
                    <% } %>
                      <div class="text-muted">
                        <%= message.message %>
                      </div>
            </div>
            <div class="modal-footer">
              <div class="w-100">
                <div class="row">
                  <div class="col">
                    <a href="/" class="btn w-100">
                      Go to dashboard
                    </a>
                  </div>

                  <% if (message.tType==='voucher' ){ %>
                    <div class="col">
                      <a href="/rent-info/<%= message.transID %>" class="btn btn-success w-100">
                        View voucher
                      </a>

                    </div>
                    <% }else{ %>
                      <div class="col">
                        <a class="btn btn-danger w-100" data-bs-dismiss="modal">
                          Close
                        </a>
                      </div>
                      <% } %>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <% } %>



        <script>
          $(document).ready(function () {
            setTimeout(function () {
              $(".alertNotif2").alert('close');
            }, 5000);
            '<% if (message && message.type ==="transac") { %>'
            $("#modal-success-trans").modal('toggle');
            ' <% } %>'
          });

        </script>